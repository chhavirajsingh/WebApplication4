@model IEnumerable<WebApplication4.Model.Employee>
@{
    ViewBag.Title = "Index";
}

<h2>Employee List</h2>
<ul>
    @foreach (var emp in Model)
    {
        <li>
            @Html.ActionLink(emp.Name, "EmployeeDetails", "Employee2", new { id = emp.Id }, null)
        </li>
    }
</ul>
@Html.ActionLink("Back to Department","Index","Employee2")